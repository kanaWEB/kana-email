{
  "default": {
    "command": "/opt/piget/email/emailSend",
    "usage" : "emailer.sh to subject body provider from [password] [smtp] [port] [protocol]",
    "args": {
      "object_key": {
        "type": "Actions",
        "modifier": "Prefix",
        "prefix": "--Id"
      },
      "To": {
      	"type": "Actions",
        "modifier": "Prefix",
        "prefix" : "--To"
      },
      "Subject": {
        "type": "Actions",
        "modifier": "Prefix",
        "prefix" : "--Subject"
      },
      "Body": {
        "type": "Actions",
        "modifier": "Prefix",
        "prefix" : "--Body"
      }
    }
  }
}